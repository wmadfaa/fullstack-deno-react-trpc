{
  "name": "@my-project-name/frontend",
  "tasks": {
    "dev": "deno run -A npm:vite",
    "build": "deno run -A npm:vite build"
  },
  "exports": {
    ".": "./src/*"
  },
  "imports": {
    "#/": "./src/",
    "+/": "./_node_modules/"
  },
  "compilerOptions": {
    "lib": ["dom"],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
